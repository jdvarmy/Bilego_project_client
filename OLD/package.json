{
  "name": "ssrbilegoreact",
  "version": "1.0.0",
  "description": "Bilego Tickets React With Server Side Rendering",
  "engines": {
    "npm": ">=3",
    "node": ">=5"
  },
  "scripts": {
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-path .eslintignore --ignore-pattern config/sitemap",
    "lint:staged": "lint-staged",
    "generate:sitemap": "node config/sitemap/sitemap.js",
    "build:clean": "rimraf ./build",
    "prod:build": "npm run generate:sitemap && cross-env NODE_ENV=production webpack --config config/webpack/server/webpack.prod.server.js",
    "dev:start": "npm run build:clean && cross-env NODE_ENV=development webpack -w --config config/webpack/server/webpack.dev.server.js",
    "server:start": "nodemon ./build/server.js"
  },
  "lint-staged": {
    "*.js": "lint:eslint"
  },
  "pre-commit": "lint:staged",
  "dependencies": {
