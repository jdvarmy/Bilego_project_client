{
  "name": "ssrbilegoreact",
  "version": "1.0.0",
  "description": "Bilego Tickets React With Server Side Rendering",
  "engines": {
    "npm": ">=3",
    "node": ">=5"
  },
  "scripts": {
    "generate:sitemap": "node config/sitemap/sitemap.js",
    "build:clean": "rimraf ./build",
    "prod:build": "npm run generate:sitemap && cross-env NODE_ENV=production webpack --config config/webpack/server/webpack.prod.server.js",
    "dev:start": "npm run build:clean && cross-env NODE_ENV=development webpack -w --config config/webpack/server/webpack.dev.server.js",
    "start": "nodemon ./build/server.js",
    "wp:dev": "npm run build:clean && cross-env NODE_ENV=development webpack -w --config webpack/config.develop.js",
    "wp:prod": "npm run build:clean && cross-env NODE_ENV=production webpack --config webpack/config.production.js --progress"
  },
  "dependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
